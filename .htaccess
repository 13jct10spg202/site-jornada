ExpiresActive On
ExpiresByType image/jpg "access"
ExpiresByType image/jpeg "access"
ExpiresByType image/gif "access"
ExpiresByType image/png "access"
ExpiresByType text/css "access"
ExpiresByType text/html "access"
ExpiresByType application/pdf "access"
ExpiresByType text/x-javascript "access"
ExpiresByType application/x-shockwave-flash "access"
ExpiresByType image/x-icon "access"
ExpiresDefault "access"
 
<IfModule mod_headers.c>
    <FilesMatch "\.(ico|gif|jpg|jpeg|png)$">
          Header set Cache-Control "max-age=31536000"
    </FilesMatch>
    <FilesMatch "\.(css|js)$">
          Header set Cache-Control "max-age=31536000"
    </FilesMatch>
</IfModule>